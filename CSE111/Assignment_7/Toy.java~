public class Toy {
  String name ="";
  int Price ="";
  public Toy (String name,int Price){
    this.name =name;
    this.Price =Price;
    System.out.prinln("A new toy has been made!");
  }
  public int updatePrice(int Price){
    this.Price =Price;
    return;
  }
  public void updateName(String name){
    System.out.prinln("Changing old name: "+this.name);
    this.name =name;
    System.out.prinln("new name: "+this.name);
  }
  public void showPrice(){
    this.Price =Price;
    System.out.prinln("price: "+Price+"Taka");   
  }
  
  
  
  
  
  